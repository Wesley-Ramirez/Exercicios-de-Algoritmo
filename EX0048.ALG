algoritmo "semnome"
var
 cont,s , n: REAL
inicio
   cont <- 0
   S <- 0
   enquanto ( cont < 7) faça
            cont <- cont + 1
            escreva("Digite o " , cont,".o numero ")
            leia(n)
            s <- s + n
   Fimenquanto
              escreval(" A soma de todos os numeros é ", s )
fimalgoritmo