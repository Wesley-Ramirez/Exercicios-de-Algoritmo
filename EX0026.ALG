algoritmo "Par ou impar"
var
   n1 , n2  : inteiro
inicio
      Escreva("Digite um numero ")
      leia(n1)
      escreva("Digite outro numero ")
      leia(n2)
      se (n1 > n2) entao
         escreva("O primeiro valor é maior")
      senao
         escreva("O segundo valor é maior")
      FimSe


fimalgoritmo