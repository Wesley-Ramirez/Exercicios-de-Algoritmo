algoritmo "EX002"
var
  Km, diaria , total  : real
inicio
  escreva("Quantos Kms voce percorreu?")
  leia(Km)
  escreva("Qual o valor da diaria do carro?")
  leia(diaria)
  total <- diaria + (km * 0.9)
  escreval("Voce rodou ", km, " Kms")
  escreval("Cada km custa R$ 0,90 centavos")
  escreval("No total , voce devera pagar ", total," Reais do aluguel do veiculo")
  

   
fimalgoritmo